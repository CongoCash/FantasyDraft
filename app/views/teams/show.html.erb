<div class="container">
  <div class="row">
    <div class="col s12">
      <h1 class="center-align"><%= @team.name %></h1>
    </div>
  </div>
  <div class="row">
    <div class="col s12 left-align">
      <hr>
      <% @team.players.each do |player| %>
          <p><%= player.name %></p>
          <hr>
      <% end %>
    </div>
  </div>
</div>